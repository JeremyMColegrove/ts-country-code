{
  "name": "ts-country-code",
  "version": "1.0.0",
  "description": "List of ISO-2 and 3 country codes",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "scripts": {
    "build": "npm run clean && npm run build:cjs && npm run build:esm && npm run build:types && npm run build:package",
    "build:package": "echo {\\\"type\\\": \\\"commonjs\\\"} > dist/cjs/package.json && echo {\\\"type\\\": \\\"module\\\"} > dist/esm/package.json",
    "build:cjs": "tsc -p config/tsconfig.cjs.json",
    "build:esm": "tsc -p config/tsconfig.esm.json",
    "build:types": "tsc -p config/tsconfig.types.json",
    "test": "jest",
    "clean": "rimraf dist",
    "push": "npm run build && npm publish"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "bin"
  ],
  "repository": {
    "type": "git",
    "url": "your-repo-here"
  },
  "license": "MIT",
  "keywords": [],
  "author": "Jeremy Colegrove",
  "bugs": {
    "url": "https://github.com/JeremyMColegrove/git-name-here/issues"
  },
  "homepage": "https://github.com/JeremyMColegrove/git-name-here#readme",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^22.2.0",
    "jest": "^29.7.0",
    "jest-coverage-badges": "^1.1.2",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.2.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "commander": "^12.1.0"
  }
}
